{
  "name": "restapi-restaraunt",
  "version": "1.0.0",
  "description": "rest api for restraunts node-express-mongodb",
  "main": "dist",
  "scripts": {
    "dev":"NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015, stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015, stage-0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions":{
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "env": {
        "node": true
    },
    "rules":{
        "no-console": 0,
        "no-unused-vars": 1
    }
},
  "keywords": [
    "node",
    "express",
    "mongodb",
    "javascript"
  ],
  "author": "capt storm 21",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2"
  }
}
